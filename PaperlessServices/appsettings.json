{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "System": "Trace",
      "Microsoft": "Trace"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PaperlessDb": "Host=postgres;Port=5432;Database=paperless;Username=postgres;Password=postgres;Include Error Detail=true"
  },
  "Storage": {
    "Minio": {
      "Endpoint": "minio:9000",
      "AccessKey": "paperless",
      "SecretKey": "paperless123",
      "BucketName": "paperless-bucket",
      "UseSsl": false
    }
  },
  "RabbitMQ": {
    "Uri": "amqp://guest:guest@rabbitmq:5672"
  },
  "Elasticsearch": {
    "Uri": "http://elasticsearch:9200",
    "IndexName": "paperless-documents"
  },
  "GenAI": {
    "Gemini": {
      "ApiKey": "",
      "Model": "gemini-2.0-flash",
      "MaxRetries": 3,
      "TimeoutSeconds": 30
    }
  }
}
